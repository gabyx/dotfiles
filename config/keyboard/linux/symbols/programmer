partial alphanumeric_keys modifier_keys
xkb_symbols "programmer" {

    include "us"
    name[Group1] = "Prog. Mx (US)";

    // To see the symbold corresponding to the keycode generated check
    // `xev` and grep inside `/usr/share/X11/xkb/keycodes directory`.

    key <AE01> { [  exclam,      1,          U1F600,  U1F4A9 ]  };     // !  1  ğŸ˜€   ğŸ’©
    key <AE02> { [  at,          2,          U1F601,  U1F4AF ]  };     // @  2  ğŸ˜   ğŸ’¯
    key <AE03> { [  numbersign,  3,          U1F602          ]  };     // #  3  ğŸ˜‚   ğŸ‰
    key <AE04> { [  dollar,      4,          U1F606,  U1F4A3 ]  };     // $  4  ğŸ˜†   ğŸ’£
    key <AE05> { [  percent,     5,          U1F605          ]  };     // %  5  ğŸ˜…
    key <AE06> { [  asciicircum, 6,          U1F607          ]  };     // ^  6  ğŸ˜‡
    key <AE07> { [  ampersand,   7,          U1F61D          ]  };     // &  7  ğŸ˜
    key <AE08> { [  asterisk,    8,          U1F914,  U1F680 ]  };     // *  8  ğŸ¤”   ğŸš€
    key <AE09> { [  parenleft,   9,          U1F62B          ]  };     // (  9  ğŸ˜«
    key <AE10> { [  parenright,  0,          U1F616          ]  };     // )  0  ğŸ˜–
    key <AE11> { [  minus,       underscore, U1F631          ]  };     // -  _  ğŸ˜±
    key <AE12> { [  equal,       plus,       U1F62D          ]  };     // =  +  ğŸ˜­

    // Switch Tilde and GreateThan (next to L-Shift)
    key <TLDE>  { [  U1F606,     U1F914  ]  };
    key <LSGT>  { [  grave,      asciitilde  ]  };

    // Right Ctrl acts as compose key.
    // key <RCTL> { [ Multi_key ] };
    // Left Ctrl acts as left Meta key.
    // key <LCTL> { [ Hyper_L ] };
    // key <RALT> { [ Meta_L ] };

    // Switch modifiers.
    // modifier_map Shift { Shift_L, Shift_R };
    // modifier_map Control { Control_L };
    // modifier_map Mod1 { Alt_L, Alt_R };
    // modifier_map Mod4 { Super_L, Super_R, Shift_L };
    // modifier_map Mod2 { Hyper_L, Hyper_R };
